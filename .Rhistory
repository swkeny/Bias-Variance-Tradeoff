n <- 1000
r <- 75
vect <- seq(n)
x <- sample(vect, r)
y <- rnorm(r, (x^3)/exp(sqrt(x/2)) +.5*x -sqrt(x), 30)
Data <- data.frame(x, y)
plot(y ~ x, Data)
